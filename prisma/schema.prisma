generator client {
  provider = "prisma-client-js"
  output   = "../generated/prisma"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Todos {
  id   Int     @id @default(autoincrement())
  text String
  done Boolean
}

model User {
  user_id  Int    @id @default(autoincrement())
  name     String
  email    String @unique
  password String
}
